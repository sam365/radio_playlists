<section id="section-1">
  <div class="head center">Charts</div>
  <div class="container row">
    <div class="col S12 M4 L4">
      <h4 class="center">Today played songs</h4>
      <table class="g-playlist-table">
        <tr>
          <th>Time</th>
          <th>Radiostation</th>
          <th>Artist</th>
          <th>Title</th>
        </tr>
        <% @today_played_songs.each do |today_played_song| %>
        <tr>
          <td><%= today_played_song.time %></td>
          <td><%= today_played_song.radiostation.name %></td>
          <td><%= today_played_song.artist.name %></td>
          <td><%= today_played_song.song.title %></td>
        </tr>
        <% end %>
      </table>
      <p><%= will_paginate @today_played_songs %></p>
    </div>
  </div>
</section>
